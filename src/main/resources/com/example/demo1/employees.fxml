<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="employeeContainer" prefHeight="653.0" prefWidth="894.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="employees.Controller">
    <children>
        <TableView fx:id="employeeTableView" layoutX="14.0" layoutY="305.0" prefHeight="252.0" prefWidth="825.0">
            <columns>
                <TableColumn fx:id="eId_Column" prefWidth="44.0" text="ID" />
                <TableColumn fx:id="eLastName_Column" prefWidth="95.0" styleClass="centered-table-column" text="LastName" />
                <TableColumn fx:id="eFirstName_Column" prefWidth="95.0" styleClass="centered-table-column" text="FirstName" />
                <TableColumn fx:id="eBirthday_Column" prefWidth="96.0" styleClass="centered-table-column" text="Birthday" />
                <TableColumn fx:id="eTitle_Column" prefWidth="98.0" styleClass="centered-table-column" text="Job Title" />
                <TableColumn fx:id="ePhone_Column" prefWidth="104.0" styleClass="centered-table-column" text="Phone" />
                <TableColumn fx:id="eJoinDate_Column" prefWidth="127.0" styleClass="centered-table-column" text="Join Date" />
                <TableColumn fx:id="eFix_Column" styleClass="centered-table-column" text="Fix" />
                <TableColumn fx:id="eDelete_Column" minWidth="0.0" prefWidth="84.0" styleClass="centered-table-column" text="Delete" />
            </columns>
        </TableView>
        <Button fx:id="addButton" layoutX="163.0" layoutY="587.0" mnemonicParsing="false" onAction="#addButton" prefHeight="37.0" prefWidth="37.0" textFill="WHITE">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
            <graphic>
                <ImageView fitHeight="37.0" fitWidth="59.0" layoutX="762.0" layoutY="668.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../icon/add.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button fx:id="saveButton" layoutX="272.0" layoutY="588.0" mnemonicParsing="false" onAction="#saveButton" prefHeight="37.0" prefWidth="43.0">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
            <graphic>
                <ImageView fitHeight="34.0" fitWidth="59.0" layoutX="914.0" layoutY="668.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../icon/save.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <ImageView fitHeight="183.0" fitWidth="297.0" layoutX="14.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../image/employee1.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="167.0" fitWidth="265.0" layoutX="311.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../image/employee2.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="180.0" fitWidth="297.0" layoutX="575.0" layoutY="45.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../image/employee3.jpg" />
            </image>
        </ImageView>
        <Button fx:id="back_btn" layoutX="405.0" layoutY="586.0" mnemonicParsing="false" onAction="#back_btn" prefHeight="37.0" prefWidth="59.0">
            <font>
                <Font name="System Bold" size="14.0" />
            </font>
            <graphic>
                <ImageView fitHeight="37.0" fitWidth="59.0" layoutX="1051.0" layoutY="668.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../../../icon/back.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</AnchorPane>
